algoritmo "Escola do elder ytalo"

var
    quant: inteiro
inicio
    escreval("-----------------------")
    escreval(" ESCOLA DO ELDER YTALO ")
    escreval("-----------------------")
    escreva("qual a quantidade de alunos a turma tem? ")
    leia(quant)
    enquanto (quant >= 1) faca
        quant <- quant + 1
        escreva(quant)
    fimenquanto
fimalgoritmo
